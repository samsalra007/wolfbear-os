#!/bin/sh



export WOLFBEAR_TARGET_DISK_ROOTFS_MNT_DIR="/mnt/target-rootfs"
export BOOT_DIR="/mnt/target-rootfs/boot"
export BOOT_EFI_DIR="/mnt/target-rootfs/boot/efi"
export BOOT_EFI_EFI_BOOT_DIR="/mnt/target-rootfs/boot/efi/EFI/BOOT"

