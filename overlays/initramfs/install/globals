export ROOTFS_INSTALLER_RESOURCES_DIR="$WOLFBEAR_INSTALLER_RESOURCES_MNT_DIR/rootfs"
export BUSYBOX_ZIP_DIR="$WOLFBEAR_INSTALLER_RESOURCES_MNT_DIR/busybox"
export BUSYBOX_ZIP_FILENAME_DIR="$WOLFBEAR_INSTALLER_RESOURCES_MNT_DIR/busybox/busybox.zip"
export QT_EVERYWHERE_ZIP_DIR="$WOLFBEAR_INSTALLER_RESOURCES_MNT_DIR/qt-everywhere"
